table
	border-collapse collapse
.clear
	clear both
.bgf7f7f7
	background #f7f7f7

.c-conference-room
	.btn-group
		padding-top 230px
		font-size 0
		text-align center
.c-add-page-public-style
	min-height 600px
	background #fff
	border-bottom 40px solid #f7f7f7
	a
		width 44%
		display inline-block
		color #666666
		font-size 28px
		text-align center
		height 90px
		line-height 88px
		letter-spacing 3px
		box-sizing border-box
		padding-left 3px
		border 1px solid #666666
		margin 0 2%
		&:hover,
		&.active
			background #8b0a42
			color #fff!important
			border-color #8b0a42
.c-date-select
	.c-child-container
		width 80%
		margin 0 auto
		.option
			padding 60px 0 40px
			text-align left
			.item
				position relative
				display inline-block
				// &:hover
				// 	.table
				// 		display block
				a
					width 232px
					height 56px
					line-height 54px
					font-size 18px
				select
					margin-left 50px
					position relative
					top -1px
					display inline-block
					box-sizing border-box
					width 324px
					height 56px
					padding-left 30px
					font-size 18px
					color #666
					border 1px solid #666666
				.table
					position absolute
					width 670px
					background #fff
					display none
					table
						width 100%
						tr,td,th
							border 1px solid #eeeeee
							text-align center
						td,th
							width 90px
							height 90px
							font-size 22px
						tr th
							color #fff
							background #8b0a42

		table
			&
			tr
			td
				text-align center
				font-size 18px
				border 1px solid #666666
			td
				width 150px
				height 150px
				&.disabled
					color #b6b6b6
					&:hover
						background none
						color #b6b6b6
						cursor default
				&.number:hover
				&.number.active
					background #8b0a42
					color #fff
					cursor pointer
		.go
			display block
			margin 110px auto
			font-size 20px
			color #fff
			width 395px
			height 56px
			line-height 54px

.c-data-people
	.c-child-page
		margin 0 auto
		width 580px
		text-align center
		.row
			padding-top 30px
			font-size 0
		h3
			padding 50px 0
			line-height 1
			font-size 30px
			color #8d0942
		label
			display block
		span
			width 15%
			display inline-block
			font-size 24px
			line-height 70px
			color #333
			vertical-align middle
			text-align left
		input
			width 85%
			padding 20px
			border 1px solid #eeeeee
			line-height 28px
			font-size 18px
			vertical-align middle
			box-sizing border-box
			&.submit
				width 90%
				display block
				margin 50px auto 50px
				height 80px
				font-size 24px
				color #fff
				line-height 78px
				padding 0
				background #8d0942
.c-vip-center
	.c-child-container
		width 95%
		margin 0 auto
		padding 55px
		box-sizing border-box
		border-bottom 1px solid #eeeeee
		.table.disabled
			table
				tr,th,td
					border 1px solid #a3a3a3
				th
					color #fff
					background #8e8e8e
				a
					color #666
					&:hover
						background none
						color #666!important
		table
			width 100%		
			tr,th,td
				text-align center
				font-size 18px
				color #666
				border 1px solid #a14b70
				font-weight normal
			td
				height 200px
			th
				height 50px
				color #fff
				background #8b0a42
			a
				width auto
				height auto
				color #8b0a42
				line-height 1
				border none
				font-size 18px
				&:hover
					background none
					color #8b0a42!important
.login
	position relative
	img
		display block
		max-width 1920px
		width 100%
		margin 0 auto
	.box
		position absolute
		left 50%
		top 50%
		transform translate(0, -50%)
		margin-left 100px
		background #fff
		width 500px
		font-size 24px
		color #8b0a42
		padding-bottom 50px
		h3
			line-height 60px
			padding 0 60px
			font-weight normal
			border-bottom 2px solid #b25c81
		.p0-60
			padding 0 60px
		.pt40
			padding-top 40px
		.input
			box-sizing border-box
			width 360px
			height 60px
			border 1px solid #f3f3f3
		.user
			padding-left 65px
			background url(../img/user.png) 13px center no-repeat
			,linear-gradient(to right,#f7f7f7 0%,#f7f7f7 100%) left top /50px 100% no-repeat
		.password
			padding-left 65px
			background url(../img/password.png) 13px center no-repeat
			,linear-gradient(to right,#f7f7f7 0%,#f7f7f7 100%) left top /50px 100% no-repeat
		.submit
			font-size 22px
			color #fff
			line-height 60px
			height 60px
			text-align center
			background #8b0a42
